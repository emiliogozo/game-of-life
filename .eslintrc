{
  "parser": "babel-eslint",
   "env": {
        "browser": true,
        "node": true
    },
  "extends": "airbnb",
  "rules": {
    "arrow-body-style": [0],
    "comma-dangle": 0,
    "indent": ["error", 2],
    "no-console": 0,
    "react/prop-types": 0,
    "react/jsx-indent-props": ["error", 2]
  }
}